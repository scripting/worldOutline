							</outline>
						<outline text="&lt;body&gt;">
							<outline text="&lt;div class=&quot;divDemoBody&quot;&gt;">
								<outline text="&lt;h1&gt;&lt;%text%&gt;&lt;/h1&gt;"/>
								<outline text="&lt;p&gt;A simple demo of a &lt;a href=&quot;http://twitter.github.com/bootstrap/javascript.html#modals&quot;&gt;modal dialog&lt;/a&gt; in Bootstrap 2.0.&lt;/p&gt;"/>
								<outline text="&lt;p&gt;It shows what a username/password dialog would look like.&lt;/p&gt;" created="Wed, 14 Mar 2012 15:07:35 GMT" pgfnum="6407"/>
								<outline text="&lt;p&gt;It doesn't actually log you into anything.&lt;/p&gt;" created="Wed, 14 Mar 2012 15:07:35 GMT" pgfnum="6407"/>
								<outline text="&lt;p&gt;Thanks to &lt;a href=&quot;http://groups.google.com/group/frontier-user/browse_thread/thread/6879b38a0ca98fbd&quot;&gt;Dan MacTough&lt;/a&gt;, we are no longer using a table for layout.&lt;/p&gt;" created="Sun, 18 Mar 2012 23:20:38 GMT" pgfnum="6513"/>
								<outline text="&lt;div class=&quot;modal hide fade form-horizontal&quot; id=&quot;usernamePasswordDialog&quot;&gt; ">
									<outline text="&lt;div class=&quot;modal-header&quot;&gt;">
										<outline text="&lt;a href=&quot;#&quot; class=&quot;close&quot; data-dismiss=&quot;modal&quot;&gt;&amp;times;&lt;/a&gt;"/>
										<outline text="&lt;h3&gt;Please enter your username and password.&lt;/h3&gt;"/>
										<outline text="&lt;/div&gt;"/>
										</outline>
									<outline text="&lt;div class=&quot;modal-body&quot;&gt;">
										<outline text="&lt;form class=&quot;form-horizontal&quot;&gt;">
											<outline text="&lt;fieldset&gt;">
												<outline text="&lt;div class=&quot;control-group&quot;&gt;">
													<outline text="&lt;label class=&quot;control-label&quot; for=&quot;username&quot;&gt;Username:&lt;/label&gt;"/>
													<outline text="&lt;div class=&quot;controls&quot;&gt;">
														<outline text="&lt;input type=&quot;text&quot; class=&quot;input-xlarge&quot; id=&quot;username&quot;/&gt;"/>
														<outline text="&lt;/div&gt;"/>
														</outline>
													<outline text="&lt;/div&gt;"/>
													</outline>
												<outline text="&lt;div class=&quot;control-group&quot;&gt;">
													<outline text="&lt;label class=&quot;control-label&quot; for=&quot;password&quot;&gt;Password:&lt;/label&gt;"/>
													<outline text="&lt;div class=&quot;controls&quot;&gt;">
														<outline text="&lt;input type=&quot;password&quot; class=&quot;input-xlarge&quot; id=&quot;password&quot;/&gt;"/>
														<outline text="&lt;/div&gt;"/>
														</outline>
													<outline text="&lt;/div&gt;"/>
													</outline>
												<outline text="&lt;/fieldset&gt;"/>
												</outline>
											<outline text="&lt;/form&gt;"/>
											</outline>
										<outline text="&lt;/div&gt;"/>
										</outline>
									<outline text="&lt;div class=&quot;modal-footer&quot;&gt;">
										<outline text="&lt;a href=&quot;#&quot; class=&quot;btn&quot; onclick=&quot;closeDialog ();&quot;&gt;Cancel&lt;/a&gt;"/>
										<outline text="&lt;a href=&quot;#&quot; class=&quot;btn btn-primary&quot; onclick=&quot;okClicked ();&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;OK&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/a&gt;"/>
										<outline text="&lt;/div&gt;"/>
										</outline>
									<outline text="&lt;/div&gt;"/>
									</outline>
								<outline text="&lt;div class=&quot;divButton&quot;&gt;">
									<outline text="&lt;a data-toggle=&quot;modal&quot; href=&quot;#usernamePasswordDialog&quot; class=&quot;btn btn-primary btn-large&quot;&gt;Launch username/password dialog&lt;/a&gt;"/>
									<outline text="&lt;/div&gt;"/>
									</outline>
								<outline text="&lt;/div&gt;"/>
								</outline>
							<outline text="&lt;/body&gt;"/>
							</outline>
						</outline>
					</outline>
