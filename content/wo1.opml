											<outline text="&lt;td valign=&quot;top&quot; align=&quot;right&quot;&gt;7/15/2004&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/td&gt;"/>
											<outline text="&lt;td&gt;&lt;img src=&quot;http://images.scripting.com/archiveScriptingCom/2004/07/15/johnsonForPresident.gif&quot; width=&quot;114&quot; height=&quot;114&quot; border=&quot;0&quot; align=&quot;right&quot; hspace=&quot;15&quot; vspace=&quot;5&quot; alt=&quot;A picture named johnsonForPresident.gif&quot; &gt;i=&quot;&quot; found=&quot;&quot; the=&quot;&quot; original=&quot;&quot; &lt;a=&quot;&quot; href=&quot;http://cyber.law.harvard.edu/blogs/gems/dave/quantumLightBreath.mp3&quot;&gt;Quantum Light Breath tape from 1990, scanned it, and linked it into the &lt;a href=&quot;http://jeru.scripting.com/&quot;&gt;Jeru site&lt;/a&gt;. \&quot;Existence is waiting for you to &lt;a href=&quot;http://images.scripting.com/archiveScriptingCom/2004/07/15/rose.jpg&quot;&gt;flower&lt;/a&gt;, waiting for your to dance your dance, to sing your song.\&quot;&lt;br&gt;&lt;br&gt;&lt;/td/&gt;"/>
											<outline text="&lt;td valign=&quot;top&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;a href=&quot;http://cyber.law.harvard.edu/blogs/gems/dave/quantumLightBreath.mp3&quot;&gt;&lt;img src=&quot;http://images.scripting.com/archiveScriptingCom/2004/12/21/sound.gif&quot; border=&quot;0&quot; width=&quot;10&quot; height=&quot;11&quot; alt=&quot;Click here to listen to podcast.&quot; &gt;&lt;=&quot;&quot; a=&quot;&quot;&gt;&lt;/td/&gt;&lt;/a&gt;"/>
