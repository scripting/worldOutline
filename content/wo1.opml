							<outline text="&lt;body&gt;">
								<outline text="&lt;div class=&quot;divDemoBody&quot;&gt;">
									<outline text="&lt;h1&gt;&lt;%text%&gt;&lt;/h1&gt;"/>
									<outline text="&lt;p&gt;A simple demo of a &lt;a href=&quot;http://twitter.github.com/bootstrap/javascript.html#modals&quot;&gt;modal dialog&lt;/a&gt; in Bootstrap 2.0.&lt;/p&gt;"/>
									<outline text="&lt;p&gt;1. Click the button. 2. Enter a new title for the window. 3. Click OK.&lt;/p&gt;" created="Wed, 14 Mar 2012 15:07:35 GMT" pgfnum="6407"/>
									<outline text="&lt;p&gt;We set the title of the window and close the dialog.&lt;/p&gt;" created="Fri, 30 Mar 2012 18:02:39 GMT" pgfnum="6938"/>
									<outline text="&lt;div id=&quot;windowTitleDialog&quot; class=&quot;modal fade&quot; tabindex=&quot;-1&quot; role=&quot;dialog&quot; aria-labelledby=&quot;windowTitleLabel&quot; aria-hidden=&quot;true&quot;&gt;" isComment="true">
										<outline text="&lt;div class=&quot;modal-header&quot;&gt;">
											<outline text="&lt;a href=&quot;#&quot; class=&quot;close&quot; data-dismiss=&quot;modal&quot;&gt;&amp;times;&lt;/a&gt;"/>
											<outline text="&lt;h3&gt;Please enter a new title for this window.&lt;/h3&gt;"/>
											<outline text="&lt;/div&gt;"/>
											</outline>
										<outline text="&lt;div class=&quot;modal-body&quot;&gt;">
											<outline text="&lt;div class=&quot;divDialogElements&quot;&gt;">
												<outline text="&lt;input class=&quot;xlarge&quot; id=&quot;xlInput&quot; name=&quot;xlInput&quot; type=&quot;text&quot; /&gt;"/>
												<outline text="&lt;/div&gt;"/>
												</outline>
											<outline text="&lt;/div&gt;"/>
											</outline>
										<outline text="&lt;div class=&quot;modal-footer&quot;&gt;">
											<outline text="&lt;a href=&quot;#&quot; class=&quot;btn&quot; onclick=&quot;closeDialog ();&quot;&gt;Cancel&lt;/a&gt;"/>
											<outline text="&lt;a href=&quot;#&quot; class=&quot;btn btn-primary&quot; onclick=&quot;okClicked ();&quot;&gt;OK&lt;/a&gt;"/>
											<outline text="&lt;/div&gt;"/>
											</outline>
										<outline text="&lt;/div&gt;"/>
										</outline>
									<outline text="" created="Wed, 31 Jul 2013 01:21:39 GMT" pgfnum="28566"/>
									<outline text="&lt;div id=&quot;windowTitleDialog&quot; class=&quot;modal fade&quot;&gt;">
										<outline text="&lt;div class=&quot;modal-dialog&quot;&gt;">
											<outline text="&lt;div class=&quot;modal-content&quot;&gt;">
												<outline text="&lt;div class=&quot;modal-header&quot;&gt;">
													<outline text="&lt;button type=&quot;button&quot; class=&quot;close&quot; data-dismiss=&quot;modal&quot; aria-hidden=&quot;true&quot;&gt;&amp;times;&lt;/button&gt;"/>
													<outline text="&lt;h4 class=&quot;modal-title&quot;&gt;Please enter a new title for this window.&lt;/h4&gt;"/>
													<outline text="&lt;/div&gt;"/>
													</outline>
												<outline text="&lt;div class=&quot;modal-body&quot;&gt;">
													<outline text="&lt;div class=&quot;divDialogElements&quot;&gt;">
														<outline text="&lt;input class=&quot;xlarge&quot; id=&quot;xlInput&quot; name=&quot;xlInput&quot; type=&quot;text&quot; /&gt;"/>
														<outline text="&lt;/div&gt;"/>
														</outline>
													<outline text="&lt;/div&gt;"/>
													</outline>
												<outline text="&lt;div class=&quot;modal-footer&quot;&gt;">
