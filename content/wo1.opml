				<outline text="Username/password dialog" created="Sun, 18 Mar 2012 01:58:58 GMT" domain="usernamepassworddialog.blorkmark.com" name="usernamepasswordDialog" pgfnum="6507" type="html">
					<outline text="&lt;html&gt;">
						<outline text="&lt;head&gt;">
							<outline text="&lt;title&gt;&lt;%text%&gt;&lt;/title&gt;"/>
							<outline text="&lt;link href=&quot;http://static.scripting.com/github/bootstrap2/css/bootstrap.css&quot; rel=&quot;stylesheet&quot;&gt;"/>
							<outline text="&lt;script src=&quot;http://static.scripting.com/github/bootstrap2/js/jquery.js&quot;&gt;&lt;/script&gt;"/>
							<outline text="&lt;script src=&quot;http://static.scripting.com/github/bootstrap2/js/bootstrap-modal.js&quot;&gt;&lt;/script&gt;"/>
							<outline text="&lt;style&gt;">
								<outline text="body {">
									<outline text="width: 60%;"/>
									<outline text="margin-left: auto;"/>
									<outline text="margin-right: auto;"/>
									<outline text="margin-top: 100px;"/>
									<outline text="}"/>
									</outline>
								<outline text=".divDemoBody p {">
									<outline text="font-size: 18px;"/>
									<outline text="line-height: 140%;"/>
									<outline text="padding-top: 12px;"/>
									<outline text="}"/>
									</outline>
								<outline text=".divButton {">
									<outline text="padding-top: 12px;"/>
									<outline text="}" created="Sun, 18 Mar 2012 22:21:23 GMT" pgfnum="6485"/>
									</outline>
								<outline text=".control-group input {">
									<outline text="font-size: 15px;"/>
									<outline text="height: 30px; "/>
									<outline text="width: 300px;"/>
									<outline text="}"/>
									</outline>
								<outline text=".control-group label {">
									<outline text="text-align: right;"/>
									<outline text="font-size: 15px;"/>
									<outline text="}"/>
									</outline>
								<outline text="&lt;/style&gt;"/>
								</outline>
							<outline text="&lt;script&gt;">
								<outline text="$(document).ready(function() {">
									<outline text="$('#usernamePasswordDialog').bind('show', function () {">
										<outline text="document.getElementById (&quot;username&quot;).value = 'bullmancuso';"/>
										<outline text="document.getElementById (&quot;password&quot;).value = 'killerinstinct';"/>
										<outline text="});"/>
										</outline>
									<outline text="});"/>
									</outline>
								<outline text="function closeDialog () {">
									<outline text="$('#usernamePasswordDialog').modal('hide'); "/>
									<outline text="};"/>
									</outline>
								<outline text="function okClicked () {">
									<outline text="var myUsername = document.getElementById (&quot;username&quot;);" created="Mon, 19 Mar 2012 13:36:34 GMT" pgfnum="6498"/>
									<outline text="var myPassword = document.getElementById (&quot;password&quot;);" created="Mon, 19 Mar 2012 13:36:34 GMT" pgfnum="6498"/>
									<outline text="alert (myUsername.value + ' -- ' + myPassword.value)" created="Mon, 19 Mar 2012 13:36:21 GMT" pgfnum="6497"/>
									<outline text="closeDialog ();"/>
									<outline text="};"/>
									</outline>
								<outline text="&lt;/script&gt;"/>
								</outline>
